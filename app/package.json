{
  "name": "{{appName}}",
  "version": "1.0.0",
  "description": "React Antd Mobx Boilerplate",
  "scripts": {
    "dev": "cross-env APP_ENV=dev NODE_ENV=development webpack-dev-server --colors --progress --config webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --config webpack.config.js && gulp build"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "cdn-bucket": "easybike-cdn",
  "cdn-directory": "{{appName}}",
  "site_url": {
    "dev": "{{dev-domain}}",
    "fat": "{{fat-domain}}",
    "uat": "{{uat-domain}}",
    "pro": "{{pro-domain}}"
  },
  "dependencies": {
    "antd": "^3.19.8",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "mobx": "^5.10.1",
    "mobx-react": "^6.1.1",
    "moment": "^2.24.0",
    "nprogress": "^0.2.0",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-utils-components": "^1.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "es6-promise": "^4.2.8",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-compat": "^3.1.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-zip": "^5.0.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.17",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "speed-measure-webpack-plugin": "^1.3.1",
    "style-loader": "^0.23.1",
